{
    "http_server" : {
        "exec" : {
            "main_class": "CaptchaModelUCloudWrapper",
            "main_file": "codes.examples.infer_captcha"
        },
        "keras" : {
            "model_dir" : "./model",
            "model_name" : "test",
            "all_one_file" : true,
            "model_arch_type" : "json"
        },
        "gunicorn" : {
            "worker" : "4",
            "worker_class" : "gevent",
            "timeout" : 60,
            "keepalive" : 60,
            "limit-request-line" : 0,
            "bind" : "0.0.0.0:8080",
            "log-level" : "error"
        }
    }
}
